{
    "name": "johnnywoode/quickussd",
    "type": "library",
    "description": "QuickUSSD package for quick ussd development and prototyping",
    "keywords": ["USSD", "QuickUssd"],
    "version": "1.0.0",
    "homepage": "https://github.com/johnnywoode/quickussd",
    "license": "MIT",
    "authors": [{
        "name": "Johnny Woode",
        "email": "johnnywoode@gmail.com",
        "homepage": "https://github.com/johnnywoode",
        "role": "Developer"
    }],
    "classmap": [
        "src/Http/controllers",
        "src/Http/migrations",
        "src/Http/seeds",
        "src/Http/models",
        "src/Http/Middleware"
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "johnnywoode\\quickussd\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "johnnywoode\\quickussd\\Test\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "kylekatarnls/update-helper": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
