{
	"name": "johnnywoode/quickussd",
	"type": "library",
	"description": "QuickUSSD package for quick ussd development and prototyping",
	"keywords": ["USSD", "QuickUssd"],
	"version": "2.0.0",
	"homepage": "https://github.com/johnnywoode/quickussd",
	"license": "MIT",
	"authors": [{
		"name": "Johnny Woode",
		"email": "johnnywoode@gmail.com",
		"homepage": "https://github.com/johnnywoode",
		"role": "Developer"
	}],
	"classmap": [
		"src/Http/Controllers",
		"src/Http/migrations",
		"src/Http/seeds",
		"src/Http/models",
		"src/Http/Middleware"
	],
	"require": {
		"php": "^8.1",
		"illuminate/support": "^5.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.1",
		"scrutinizer/ocular": "~1.1"
	},
	"autoload": {
		"psr-4": {
			"johnnywoode\\quickussd\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"johnnywoode\\quickussd\\Test\\": "tests/"
		}
	},
	"scripts": {
		"test": "phpunit"
	},
	"extra": {
		"branch-alias": {
			"dev-main": "v2.0.0"
		}
	},
	"config": {
		"allow-plugins": {
			"kylekatarnls/update-helper": true
		}
	}
}
